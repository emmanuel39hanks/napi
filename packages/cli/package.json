{
  "name": "@nanoapi.io/nanoapi",
  "version": "1.0.5",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "napi": "./dist/index.js"
  },
  "pkg": {
    "assets": [
      "../../node_modules/tree-sitter/**/*",
      "../../node_modules/openai/**/*",
      "dist/**/*"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "binaries"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "npm:publish": "npm publish",
    "binaries:build": "pkg ./package.json"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^18.19.54",
    "@types/yargs": "^17.0.33",
    "nodemon": "^3.1.7",
    "pkg": "^5.8.1",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "dependency-tree": "^11.0.1",
    "express": "^4.21.1",
    "http-proxy-middleware": "^3.0.3",
    "openai": "^4.67.2",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.0",
    "tree-sitter-typescript": "^0.23.0",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  }
}
